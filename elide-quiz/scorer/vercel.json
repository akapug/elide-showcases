{
  "rewrites": [
    {
      "source": "/api/score",
      "destination": "/api/score"
    },
    {
      "source": "/api/leaderboard",
      "destination": "/api/leaderboard"
    }
  ],
  "headers": [
    {
      "source": "/questions.md",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/markdown; charset=utf-8"
        }
      ]
    },
    {
      "source": "/answers.md",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/markdown; charset=utf-8"
        }
      ]
    },
    {
      "source": "/answers-human.md",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/markdown; charset=utf-8"
        }
      ]
    }
  ],
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false
}

