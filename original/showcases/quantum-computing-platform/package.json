  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "python": {
    "dependencies": {
      "qiskit": ">=0.45.0",
      "qiskit-aer": ">=0.13.0",
