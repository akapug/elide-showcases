{
  "name": "@elide-showcases/real-time-analytics-engine",
  "version": "1.0.0",
  "description": "Real-time analytics engine processing 50K events/sec with TypeScript ingestion + Python pandas/polars analytics",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/server.ts",
    "dev": "ts-node-dev --respawn src/server.ts",
    "test": "ts-node tests/analytics.test.ts && ts-node tests/ingestion.test.ts && ts-node tests/windowing.test.ts",
    "benchmark": "ts-node benchmarks/throughput.bench.ts && ts-node benchmarks/latency.bench.ts",
    "example:streaming": "ts-node examples/streaming-analytics.ts",
    "example:batch": "ts-node examples/batch-analytics.ts",
    "example:dashboard": "ts-node examples/dashboard-query.ts"
  },
  "keywords": [
    "analytics",
    "real-time",
    "streaming",
    "pandas",
    "polars",
    "dataframe",
    "fastify",
    "polyglot",
    "typescript",
    "python",
    "zero-copy",
    "high-performance"
  ],
  "author": "Elide Showcases",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/node": "^20.0.0",
    "fastify": "^4.25.0",
    "@fastify/cors": "^8.5.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elide-dev/elide-showcases.git",
    "directory": "showcases/real-time-analytics-engine"
  }
}
