{
  "name": "@elide/computer-vision-platform",
  "version": "1.0.0",
  "description": "Production Computer Vision Platform - OpenCV, YOLO, dlib in TypeScript via Elide",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "elide run src/index.ts",
    "build": "elide build src/index.ts",
    "demo:detection": "elide run examples/object-detection-demo.ts",
    "demo:face": "elide run examples/face-recognition-demo.ts",
    "demo:video": "elide run examples/video-processing-demo.ts",
    "demo:tracking": "elide run examples/object-tracking-demo.ts",
    "demo:segmentation": "elide run examples/semantic-segmentation-demo.ts",
    "demo:pose": "elide run examples/pose-demo.ts",
    "demo:ocr": "elide run examples/ocr-demo.ts",
    "demo:enhancement": "elide run examples/enhancement-demo.ts",
    "benchmark": "elide run benchmarks/performance-benchmark.ts",
    "test": "elide test"
  },
  "keywords": [
    "elide",
    "polyglot",
    "computer-vision",
    "opencv",
    "yolo",
    "face-recognition",
    "object-detection",
    "video-processing",
    "real-time",
    "typescript",
    "python"
  ],
  "dependencies": {
    "@elide/runtime": "^1.0.0"
  },
  "pythonDependencies": {
    "opencv-python": "^4.8.0",
    "torch": "^2.0.0",
    "torchvision": "^0.15.0",
    "numpy": "^1.24.0",
    "pillow": "^10.0.0",
    "dlib": "^19.24.0",
    "face-recognition": "^1.3.0",
    "mediapipe": "^0.10.0",
    "pytesseract": "^0.3.10",
    "scikit-image": "^0.21.0",
    "scipy": "^1.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "elide": ">=1.0.0"
  },
  "author": "Elide Team",
  "license": "MIT"
}
