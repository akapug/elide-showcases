{
  "name": "@elide/cybersecurity-platform",
  "version": "1.0.0",
  "description": "Enterprise cybersecurity platform demonstrating Elide's polyglot TypeScript + Python integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "demo": "node dist/examples/security-demo.js",
    "demo:intrusion": "node dist/examples/security-demo.js intrusion",
    "demo:malware": "node dist/examples/security-demo.js malware",
    "demo:hunting": "node dist/examples/security-demo.js hunting",
    "demo:full": "node dist/examples/security-demo.js full",
    "benchmark": "node dist/benchmarks/detection-performance.js",
    "benchmark:packets": "node dist/benchmarks/detection-performance.js packets",
    "benchmark:detection": "node dist/benchmarks/detection-performance.js detection",
    "benchmark:malware": "node dist/benchmarks/detection-performance.js malware",
    "test": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "test:performance": "jest --testPathPattern=performance",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "elide",
    "polyglot",
    "cybersecurity",
    "network-security",
    "intrusion-detection",
    "malware-analysis",
    "threat-intelligence",
    "siem",
    "soar",
    "packet-analysis",
    "vulnerability-scanning"
  ],
  "author": "Elide Team",
  "license": "MIT",
  "dependencies": {
    "python:scapy": "^2.5.0",
    "python:scikit-learn": "^1.3.0",
    "python:numpy": "^1.24.0",
    "python:pandas": "^2.0.0",
    "python:cryptography": "^41.0.0",
    "python:requests": "^2.31.0",
    "python:pefile": "^2023.2.7",
    "python:yara-python": "^4.3.0",
    "python:psutil": "^5.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.2.0",
    "eslint": "^8.50.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "prettier": "^3.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.9.0"
  }
}
