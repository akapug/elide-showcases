{
  "name": "@elide/anomaly-detection-engine",
  "version": "1.0.0",
  "description": "Production-ready real-time anomaly detection engine with TypeScript + Python scikit-learn",
  "main": "api/server.ts",
  "type": "module",
  "scripts": {
    "start": "tsx api/server.ts",
    "dev": "tsx watch api/server.ts",
    "build": "tsc",
    "test": "tsx tests/api-test.ts",
    "test:ml": "python3 -m pytest tests/ml-test.py -v",
    "test:integration": "tsx tests/integration-test.ts",
    "test:all": "npm test && npm run test:ml && npm run test:integration",
    "benchmark": "tsx benchmarks/latency-benchmark.ts",
    "benchmark:algorithms": "tsx benchmarks/algorithm-comparison.ts",
    "benchmark:accuracy": "tsx benchmarks/accuracy-benchmark.ts",
    "example:iot": "tsx examples/iot-monitoring.ts",
    "example:security": "tsx examples/security-threats.ts",
    "example:business": "tsx examples/business-metrics.ts",
    "train": "python3 ml/trainer.py",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,js,json,md}\""
  },
  "keywords": [
    "anomaly-detection",
    "machine-learning",
    "real-time",
    "scikit-learn",
    "isolation-forest",
    "outlier-detection",
    "time-series",
    "monitoring",
    "alerting"
  ],
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.9",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "prettier": "^3.1.1",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.8"
  },
  "author": "Elide Team",
  "license": "MIT"
}
