{
  "name": "@elide-showcases/llm-agent-framework",
  "version": "1.0.0",
  "description": "Multi-agent LLM orchestration with TypeScript coordination and Python agents - Tier S AI/ML Showcase",
  "main": "api/server.ts",
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm api/server.ts",
    "dev": "nodemon --exec node --loader ts-node/esm api/server.ts",
    "test": "vitest run tests/orchestration-test.ts",
    "test:agents": "python3 -m pytest tests/agent-test.py -v",
    "test:all": "npm test && npm run test:agents",
    "benchmark": "node --loader ts-node/esm benchmarks/routing-benchmark.ts",
    "benchmark:tasks": "node --loader ts-node/esm benchmarks/task-benchmark.ts",
    "example:code": "node --loader ts-node/esm examples/code-generation.ts",
    "example:research": "node --loader ts-node/esm examples/research.ts",
    "example:collaboration": "node --loader ts-node/esm examples/collaboration.ts"
  },
  "keywords": ["elide", "polyglot", "llm", "agents", "orchestration", "openai", "anthropic"],
  "author": "Elide Showcases",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pino": "^8.16.1",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "vitest": "^1.0.4"
  }
}
