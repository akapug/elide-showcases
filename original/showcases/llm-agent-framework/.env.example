# Server
PORT=3000
NODE_ENV=development
LOG_LEVEL=info

# LLM Providers
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-...
TOGETHER_API_KEY=...

# Agent Configuration
DEFAULT_MODEL=gpt-4-turbo
MAX_TOKENS=4000
TEMPERATURE=0.7

# Performance
ROUTING_TIMEOUT=200
TASK_TIMEOUT=5000
MAX_AGENTS_PER_TASK=5

# Task Queue
MAX_CONCURRENT_TASKS=10
TASK_RETRY_LIMIT=3
