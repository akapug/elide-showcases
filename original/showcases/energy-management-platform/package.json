{
  "name": "@elide-showcases/energy-management-platform",
  "version": "1.0.0",
  "description": "Production-ready smart grid management system with renewable energy forecasting and optimization",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "test": "vitest",
    "test:integration": "vitest --config vitest.integration.config.ts",
    "test:performance": "tsx benchmarks/energy-perf.ts",
    "train:load": "tsx scripts/train-load-model.ts",
    "train:solar": "tsx scripts/train-solar-model.ts",
    "train:wind": "tsx scripts/train-wind-model.ts",
    "train:price": "tsx scripts/train-price-model.ts",
    "demo": "tsx examples/energy-demo.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "energy",
    "smart-grid",
    "renewable-energy",
    "forecasting",
    "optimization",
    "demand-response",
    "battery",
    "solar",
    "wind",
    "machine-learning",
    "python",
    "typescript",
    "elide"
  ],
  "author": "Elide Team",
  "license": "MIT",
  "dependencies": {
    "@elide/runtime": "^1.0.0",
    "python:pandas": "^2.0.0",
    "python:numpy": "^1.24.0",
    "python:scikit-learn": "^1.3.0",
    "python:scipy": "^1.11.0",
    "python:pvlib": "^0.10.0",
    "python:statsmodels": "^0.14.0",
    "python:prophet": "^1.1.0",
    "python:xgboost": "^2.0.0",
    "python:tensorflow": "^2.13.0",
    "python:keras": "^2.13.0",
    "python:matplotlib": "^3.7.0",
    "python:seaborn": "^0.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.2.0",
    "tsx": "^3.12.0",
    "vitest": "^0.34.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.0"
  }
}
