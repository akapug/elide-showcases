# Server Configuration
PORT=3000
HOST=0.0.0.0
NODE_ENV=production

# ETL Configuration
MAX_BATCH_SIZE=10000
VALIDATION_TIMEOUT=5000
ENABLE_ZERO_COPY=true
CHUNK_SIZE=1000

# Data Sources
INPUT_PATH=./data/input
OUTPUT_PATH=./data/output
TEMP_PATH=./data/temp

# Performance
MAX_WORKERS=4
MEMORY_LIMIT_MB=4096
ENABLE_PARALLEL=true

# Logging
LOG_LEVEL=info
LOG_FORMAT=json
