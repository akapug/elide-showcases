PORT=3001
NODE_ENV=development
LOG_LEVEL=info

# Fraud Detection Thresholds
FRAUD_SCORE_THRESHOLD=0.75
HIGH_RISK_THRESHOLD=0.90
AUTO_DECLINE_THRESHOLD=0.95

# Performance Targets
MAX_DETECTION_TIME_MS=5
ENABLE_ML_PREDICTION=true

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# Feature Flags
ENABLE_VELOCITY_CHECKS=true
ENABLE_GEOLOCATION_CHECKS=true
ENABLE_DEVICE_FINGERPRINT=true
