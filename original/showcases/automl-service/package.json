{
  "name": "@elide-showcases/automl-service",
  "version": "1.0.0",
  "description": "Automated ML platform with TypeScript UI and Python auto-sklearn/Optuna - Tier S AI/ML Showcase",
  "main": "api/server.ts",
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm api/server.ts",
    "dev": "nodemon --exec node --loader ts-node/esm api/server.ts",
    "ui": "cd ui && npm run dev",
    "test": "vitest run tests/",
    "test:all": "npm test",
    "benchmark": "node --loader ts-node/esm benchmarks/automl-benchmark.ts",
    "example:classification": "node --loader ts-node/esm examples/classification.ts",
    "example:regression": "node --loader ts-node/esm examples/regression.ts",
    "mlflow": "mlflow ui --port 5000"
  },
  "keywords": ["elide", "polyglot", "automl", "auto-sklearn", "optuna", "mlflow"],
  "author": "Elide Showcases",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pino": "^8.16.1",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1",
    "ws": "^8.14.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "vitest": "^1.0.4",
    "vite": "^5.0.0"
  }
}
