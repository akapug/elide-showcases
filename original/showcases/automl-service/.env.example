# Server
PORT=3000
NODE_ENV=development
LOG_LEVEL=info

# MLflow
MLFLOW_PORT=5000
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_ARTIFACT_ROOT=./mlruns

# AutoML
MAX_EXPERIMENT_TIME=3600
MAX_MODELS=200
DEFAULT_ENGINE=auto-sklearn

# Resources
MAX_MEMORY_GB=16
N_JOBS=-1

# UI
UI_PORT=3001
ENABLE_UI=true
