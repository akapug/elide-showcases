{
  "name": "@elide-showcases/fastify",
  "version": "1.0.0",
  "description": "Fastify web framework on Elide - Fast and low overhead with polyglot superpowers",
  "main": "src/fastify.ts",
  "type": "module",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/fastify.test.ts",
    "test:router": "jest tests/router.test.ts",
    "test:schema": "jest tests/schema.test.ts",
    "test:hooks": "jest tests/hooks.test.ts",
    "test:plugins": "jest tests/plugins.test.ts",
    "example:basic": "elide run examples/basic-server.ts",
    "example:schema": "elide run examples/schema-validation.ts",
    "example:hooks": "elide run examples/hooks-example.ts",
    "example:plugins": "elide run examples/plugins-example.ts",
    "example:polyglot": "elide run --polyglot examples/polyglot-validation.ts",
    "example:performance": "elide run examples/high-performance-api.ts",
    "build": "elide build src/fastify.ts",
    "build:native": "elide build --native src/fastify.ts -o fastify-native",
    "lint": "eslint src tests examples --ext .ts",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts' 'examples/**/*.ts'",
    "benchmark": "cd benchmarks && ./run-all-benchmarks.sh",
    "benchmark:simple": "wrk -t4 -c100 -d30s http://localhost:3000/health",
    "benchmark:throughput": "autocannon -c 100 -d 30 http://localhost:3000/api/hello"
  },
  "keywords": [
    "fastify",
    "elide",
    "graalvm",
    "web-framework",
    "http",
    "fast",
    "low-overhead",
    "polyglot",
    "schema-validation",
    "performance",
    "serverless",
    "microservices"
  ],
  "author": "Elide Team",
  "license": "MIT",
  "dependencies": {
    "events": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "ts-jest": "^29.1.1",
    "autocannon": "^7.12.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "elide": ">=1.0.0-alpha.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elide-dev/showcases.git",
    "directory": "converted/tier1-frameworks/fastify"
  },
  "bugs": {
    "url": "https://github.com/elide-dev/showcases/issues"
  },
  "homepage": "https://github.com/elide-dev/showcases/tree/main/converted/tier1-frameworks/fastify",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
